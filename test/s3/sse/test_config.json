{
  "description": "Configuration for S3 SSE Integration Tests",
  "endpoint": "http://127.0.0.1:8333",
  "access_key": "some_access_key1", 
  "secret_key": "some_secret_key1",
  "region": "us-east-1",
  "bucket_prefix": "test-sse-",
  "use_ssl": false,
  "skip_verify_ssl": true,
  "test_scenarios": {
    "basic_sse_c": {
      "description": "Basic SSE-C encryption/decryption tests",
      "enabled": true,
      "test_data_sizes": [0, 1, 16, 32, 100, 1024, 8192, 65536, 1048576]
    },
    "basic_sse_kms": {
      "description": "Basic SSE-KMS encryption/decryption tests", 
      "enabled": true,
      "test_kms_key_ids": ["test-key-123", "test-key-456", "alias/test-key"]
    },
    "copy_operations": {
      "description": "SSE object copy operations",
      "enabled": true,
      "scenarios": ["ssec_to_ssec", "ssec_to_plain", "ssekms_to_ssekms", "plain_to_ssec"]
    },
    "multipart_uploads": {
      "description": "SSE multipart upload tests",
      "enabled": true,
      "part_sizes": [5242880, 10485760],
      "max_parts": 10
    },
    "error_conditions": {
      "description": "SSE error handling tests",
      "enabled": true,
      "scenarios": ["invalid_key_length", "wrong_key", "empty_kms_key", "malformed_headers"]
    },
    "performance_tests": {
      "description": "SSE performance benchmarks",
      "enabled": false,
      "data_sizes": [1048576, 10485760, 104857600],
      "iterations": 10
    }
  },
  "kms_config": {
    "type": "local",
    "test_keys": [
      {
        "key_id": "test-key-123",
        "algorithm": "AES-256",
        "description": "Primary test key"
      },
      {
        "key_id": "test-key-456", 
        "algorithm": "AES-256",
        "description": "Secondary test key"
      },
      {
        "key_id": "alias/test-key",
        "algorithm": "AES-256", 
        "description": "Alias test key"
      }
    ]
  },
  "test_environment": {
    "master_port": 9333,
    "volume_port": 8080,
    "filer_port": 8888,
    "s3_port": 8333,
    "volume_max_size_mb": 50,
    "cleanup_on_failure": true,
    "debug_logs": false
  },
  "validation": {
    "verify_content_length": true,
    "verify_encryption_headers": true,
    "verify_metadata_persistence": true,
    "check_data_integrity": true
  }
}
