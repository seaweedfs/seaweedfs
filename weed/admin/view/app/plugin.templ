    <style>
        #plugin-page[data-plugin-page="overview"] .plugin-section-configuration,
        #plugin-page[data-plugin-page="overview"] .plugin-section-queue,
        #plugin-page[data-plugin-page="overview"] .plugin-section-detection,
        #plugin-page[data-plugin-page="overview"] .plugin-section-execution {
            display: none;
        }

        #plugin-page[data-plugin-page="configuration"] .plugin-section-overview,
        #plugin-page[data-plugin-page="configuration"] .plugin-section-queue,
        #plugin-page[data-plugin-page="configuration"] .plugin-section-detection,
        #plugin-page[data-plugin-page="configuration"] .plugin-section-execution {
            display: none;
        }

        #plugin-page[data-plugin-page="queue"] .plugin-section-overview,
        #plugin-page[data-plugin-page="queue"] .plugin-section-configuration,
        #plugin-page[data-plugin-page="queue"] .plugin-section-detection,
        #plugin-page[data-plugin-page="queue"] .plugin-section-execution {
