            margin-right: 0.25rem;
        }

        .plugin-job-progress {
            min-width: 120px;
        }

        .plugin-detection-item {
            border: 1px solid #dee2e6;
            border-radius: 6px;
            padding: 0.5rem 0.75rem;
            margin-bottom: 0.5rem;
            background: #fbfcfd;
        }
