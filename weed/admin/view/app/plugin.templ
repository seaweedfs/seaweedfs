package app

templ Plugin(page string) {
    {{
        currentPage := page
        if currentPage == "" {
            currentPage = "overview"
        }
    }}
    <div class="container-fluid" id="plugin-page" data-plugin-page={ currentPage }>
        <div class="row mb-4">
            <div class="col-12">
                <div class="d-flex justify-content-between align-items-center flex-wrap gap-2">
                    <div>
                        <h2 class="mb-0"><i class="fas fa-plug me-2"></i>Workers</h2>
                        if currentPage == "overview" {
                            <p class="text-muted mb-0">Cluster-wide worker status, inventory, and scheduler state.</p>
                        } else if currentPage == "configuration" {
                            <p class="text-muted mb-0">Configure job types, refresh schemas, and run on-demand workflows.</p>
                        } else if currentPage == "queue" {
                            <p class="text-muted mb-0">Track currently assigned and running jobs waiting in the execution pipeline.</p>
                        } else if currentPage == "execution" {
                            <p class="text-muted mb-0">Track execution jobs, states, and runtime activity across workers.</p>
                        } else {
                            <p class="text-muted mb-0">Track detection decisions and activity streams across all worker job types.</p>
                        }
                    </div>
                    <div class="btn-group">
                        <button type="button" class="btn btn-outline-secondary" id="plugin-refresh-all-btn">
                            <i class="fas fa-sync-alt me-1"></i>Refresh
                        </button>
                    </div>
                </div>
            </div>
        </div>

        <div class="row mb-3">
            <div class="col-12">
                <ul class="nav nav-tabs">
                    <li class="nav-item">
                        <a class={ "nav-link", templ.KV("active", currentPage == "overview") } href="/plugin">
                            <i class="fas fa-chart-line me-1"></i>Overview
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class={ "nav-link", templ.KV("active", currentPage == "configuration") } href="/plugin/configuration">
                            <i class="fas fa-sliders-h me-1"></i>Configuration
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class={ "nav-link", templ.KV("active", currentPage == "queue") } href="/plugin/queue">
                            <i class="fas fa-list me-1"></i>Job Queue
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class={ "nav-link", templ.KV("active", currentPage == "detection") } href="/plugin/detection">
                            <i class="fas fa-search me-1"></i>Detection Jobs
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class={ "nav-link", templ.KV("active", currentPage == "execution") } href="/plugin/execution">
                            <i class="fas fa-tasks me-1"></i>Execution Jobs
                        </a>
                    </li>
                </ul>
            </div>
        </div>

        <div class="plugin-section plugin-section-overview">
            <div class="row mb-4">
                <div class="col-md-4 mb-3">
                    <div class="card border-info h-100">
                        <div class="card-body text-center">
                            <div class="text-uppercase text-muted small mb-1">Workers</div>
                            <h4 class="mb-0" id="plugin-status-workers">0</h4>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 mb-3">
                    <div class="card border-warning h-100">
                        <div class="card-body text-center">
                            <div class="text-uppercase text-muted small mb-1">Active Jobs</div>
                            <h4 class="mb-0" id="plugin-status-active-jobs">0</h4>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 mb-3">
                    <div class="card border-success h-100">
                        <div class="card-body text-center">
                            <div class="text-uppercase text-muted small mb-1">Activities (recent)</div>
                            <h4 class="mb-0" id="plugin-status-activities">0</h4>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row mb-4">
                <div class="col-12">
                    <div class="card shadow-sm">
                        <div class="card-header">
                            <h5 class="mb-0"><i class="fas fa-chart-bar me-2"></i>Per Job Type Summary</h5>
                        </div>
                        <div class="card-body p-0">
                            <div class="table-responsive">
                                <table class="table table-sm table-hover mb-0">
                                    <thead class="table-light">
                                        <tr>
                                            <th>Job Type</th>
                                            <th>Active Jobs</th>
                                            <th>Recent Activities</th>
                                        </tr>
                                    </thead>
                                    <tbody id="plugin-jobtype-summary-body">
                                        <tr><td colspan="3" class="text-muted text-center py-3">Loading...</td></tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row mb-4">
                <div class="col-12">
                    <div class="card shadow-sm">
                        <div class="card-header d-flex justify-content-between align-items-center flex-wrap gap-2">
