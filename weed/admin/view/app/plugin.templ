        #plugin-page[data-plugin-page="detection"] .plugin-section-overview,
        #plugin-page[data-plugin-page="detection"] .plugin-section-configuration,
        #plugin-page[data-plugin-page="detection"] .plugin-section-queue,
        #plugin-page[data-plugin-page="detection"] .plugin-section-execution {
